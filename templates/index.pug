doctype html
html(lang='en')
  head
    title
      block title
    link(href='http://fonts.googleapis.com/icon?family=Material+Icons', rel='stylesheet')
    // Import materialize.css
    link(type='text/css', rel='stylesheet', href='static/css/materialize.min.css', media='screen,projection')
    // Let browser know website is optimized for mobile
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    // Import jQuery before materialize.js
    script(type='text/javascript', src='static/js/jquery-3.2.1.js')
    script(type='text/javascript', src='static/js/materialize.min.js')
    script
      | $(document).ready(function() {
      | $('select').material_select();
      | });
      | $(document).ready(function(){
      | $('.tooltipped').tooltip({delay: 50});
      | });
      | $(document).ready(function(){
      | $('.collapsible').collapsible();
      | });
  body
    nav
      .nav-wrapper
        a.brand-logo.center(href='#!') Logo
        ul.left.hide-on-med-and-down
          li.active
            a(href="{{url_for('index')}}") Home
          li.active
            a(href="{{url_for('selector1')}}") Budget-Selector
    block body